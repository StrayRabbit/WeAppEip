<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    @*<meta name="viewport" content="initial-scale=1.0, maximum-scale=1.0, user-scalable=no">*@
    <title>@ViewBag.Title</title>
    <!-- Tell the browser to be responsive to screen width -->
    @*<meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">*@
    <link href="~/css/bootstrap.min.css" rel="stylesheet" />
    <!-- Font Awesome -->
    <link href="~/css/font-awesome.css" rel="stylesheet" />
    <!-- Ionicons -->
    <link href="~/Ionicons/css/ionicons.min.css" rel="stylesheet" />
    <!-- Theme style -->
    <link href="~/AdminLTE/css/AdminLTE.min.css" rel="stylesheet" />
    <!-- AdminLTE Skins. We have chosen the skin-blue for this starter
    page. However, you can choose any other skin. Make sure you
    apply the skin class to the body tag so the changes take effect. -->
    <link href="~/AdminLTE/css/skins/skin-blue.min.css" rel="stylesheet" />
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <!-- Google Font -->
    <link href="~/AdminLTE/css/GoogleFont.css" rel="stylesheet" />
    <link href="~/Dialog/css/ui-dialog.css" rel="stylesheet" />
    <link href="~/sweetalert/sweetalert.css" rel="stylesheet" />
    <link href="~/FormValidator/bootstrapValidator.css" rel="stylesheet" />
    @RenderSection("styles", false)
    <script src="~/js/jquery-3.3.1.min.js"></script>
</head>
<body>
    @RenderBody()
    <!-- Bootstrap 3.3.7 -->
    <script src="~/js/bootstrap.min.js"></script>
    <!-- AdminLTE App -->
    <script src="~/AdminLTE/js/adminlte.min.js"></script>
    <script src="~/Dialog/js/dialog-plus-min.js"></script>
    <script src="~/js/jquery.serializeJSON.min.js"></script>
    <script src="~/js/common.js"></script>
    <script src="~/FormValidator/bootstrapValidator.min.js"></script>
    <script src="~/FormValidator/zh_CN.js"></script>
    <script src="~/Dialog/js/dialog-plus-min.js"></script>
    <script src="~/sweetalert/sweetalert.min.js"></script>
    @RenderSection("scripts", false)
</body>
</html>